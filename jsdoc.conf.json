{
    "docdash": {
        "collapse": false,
        "menu":{
            "Github repo":{
                "href":"https://github.com/nestauk/svizzle",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        },
        "meta": {
            "title": "Svizzle",
            "description": "Documentation for Svizzle",
            "keyword": "data visualisation, dataviz, svelte, components, utilities",
            "favicon": "./doc/cocktail_32.png"
        },
        "search": true
    },
    "opts": {
        "encoding": "utf8",
        "lenient": true,
        "private": false,
        "recurse": true,
        "template": "./node_modules/docdash"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "source": {
        "include": [
            "packages/tools",
            "packages/components"
        ],
        "exclude": [
            "packages/tools/atlas"
        ],
        "excludePattern": "(node_modules|dist|index.js|rollup.config.js|spec.js)"
    },
    "tags": {
        "allowUnknownTags": false
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        },
        "applicationName": "Svizzle",
        "footer": "Nesta © 2022 (https://www.nesta.org.uk)",
        "copyright": "Nesta © 2022",
        "linenums": true
    }
}
