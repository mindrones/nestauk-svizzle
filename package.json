{
    "devDependencies": {
        "jsdoc": "^3.5.5",
        "lerna": "^3.13.0"
    },
    "dependencies": {},
    "author": "nestauk",
    "name": "svizzle",
    "private": true,
    "scripts": {
        "cleanall": "npm run cleanpacks && npm run cleanpacklocks",
        "cleandist": "rimraf packages/**/dist",
        "cleannodmod": "rimraf node_modules",
        "cleanpacklocks": "rimraf packages/**/package-lock.json",
        "cleanpacks": "npm run cleandist && npm run lernaclean",
        "doc": "rimraf docs && jsdoc -c jsdoc.conf.json",
        "lernaboot": "lerna bootstrap --hoist",
        "lernabuild": "lerna run build",
        "lernaclean": "lerna clean",
        "lernacleanboot": "lerna clean && npm run lernaboot",
        "lernatest": "lerna run test",
        "prepublishOnly": "npm run cleanall && npm run lernaboot && npm run lernatest && npm run lernabuild"
    }
}
