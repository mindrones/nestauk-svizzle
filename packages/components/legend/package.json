{
	"author": "nestauk (https://www.nesta.org.uk/)",
	"browser": "dist/index.js",
	"bugs": {
		"url": "https://github.com/nestauk/svizzle/issues"
	},
	"dependencies": {
		"@svizzle/dom": "^0.4.1",
		"@svizzle/geometry": "^0.3.1",
		"@svizzle/histogram": "^0.2.0",
		"@svizzle/utils": "^0.9.0",
		"d3-scale": "^3.2.1",
		"lamb": "^0.59.2"
	},
	"description": "Legend Svelte components",
	"devDependencies": {
		"@svizzle/dev": "^0.4.1",
		"@svizzle/file": "^0.5.0",
		"eslint": "^6.8.0",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-svelte3": "^2.7.3",
		"esm": "^3.2.25",
		"mkdirp": "^1.0.4",
		"mocha": "^5.2.0",
		"rimraf": "^3.0.2",
		"rollup": "^2.21.0",
		"rollup-plugin-analyzer": "^3.1.2",
		"rollup-plugin-buble": "^0.19.8",
		"rollup-plugin-cleanup": "^3.1.1",
		"rollup-plugin-commonjs": "^10.0.2",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-svelte": "^5.2.3",
		"rollup-plugin-terser": "^5.2.0",
		"svelte": "^3.24.0"
	},
	"files": [
		"!*.spec.js",
		"!pkg/*",
		"dist/*",
		"LICENSE",
		"README.md",
		"src/**/*.js",
		"src/**/*.svelte"
	],
	"homepage": "https://github.com/nestauk/svizzle",
	"keywords": [
		"chart",
		"color scale",
		"color",
		"component",
		"data visualization",
		"scale",
		"svelte"
	],
	"license": "MIT",
	"main": "dist/index.js",
	"module": "src/index.js",
	"name": "@svizzle/legend",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"directory": "packages/components/legend",
		"type": "git",
		"url": "github:nestauk/svizzle"
	},
	"scripts": {
		"build": "rimraf dist && rollup -c",
		"buildpack": "npm run build && npm run pack",
		"bumppack": "npm run setprerelease && npm run pack",
		"lint": "eslint rollup.config.js 'src/*.{js,svelte}'",
		"pack": "mkdirp pkg && rimraf pkg/* && dirname=${PWD##*/} && tar -cvz -f pkg/${npm_package_version}.tar.gz -C .. ${dirname}/dist/ ${dirname}/LICENSE ${dirname}/package.json ${dirname}/src/",
		"setpreminor": "npm --no-git-tag-version version preminor --preid=dev",
		"setprerelease": "npm --no-git-tag-version version prerelease --preid=dev"
	},
	"sideEffects": false,
	"version": "0.1.0"
}
