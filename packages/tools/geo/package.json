	},
	"config": {
		"name": "geo",
		"preid": "dev"
	},
	"dependencies": {
		"@svizzle/file": "^0.13.0",
		"@turf/bbox": "6.4.0",
		"@turf/centroid": "6.4.0",
		"@turf/helpers": "6.4.0",
		"@turf/truncate": "6.4.0",
		"commander": "^7.2.0",
		"lamb": "^0.60.0",
		"topojson-client": "^3.1.0"
	},
	"description": "Svizzle Geo contains geography related helpers.",
	"devDependencies": {
		"@svizzle/utils": "^0.17.0",
		"eslint": "^7.24.0",
		"eslint-plugin-import": "^2.18.2",
		"esm": "^3.2.25",
		"mocha": "^8.3.2"
	},
	"files": [
		"index.js",
		"src/**/*.js",
		"!*.spec.js",
		"!*.todo.js",
		"CHANGELOG.md",
		"LICENSE",
		"README.md"
	],
	"homepage": "https://github.com/nestauk/svizzle",
	"keywords": [
		"Geojson",
		"functional programming",
		"utility"
	],
	"license": "MIT",
	"module": "index.js",
	"name": "@svizzle/geo",
	"repository": {
		"type": "git",
		"url": "github:nestauk/svizzle",
		"directory": "packages/tools/geo"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"link": "npm link",
		"lint": "eslint --max-warnings=0 src/**/*.js",
		"outdated": "npm outdated",
		"setpreminor": "npm --no-git-tag-version version preminor --preid=${npm_package_config_preid}",
		"setprepatch": "npm --no-git-tag-version version prepatch --preid=${npm_package_config_preid}",
		"setprerelease": "npm --no-git-tag-version version prerelease --preid=${npm_package_config_preid}",
		"test": "mocha -r esm --recursive src/modules",
		"unlink": "npm unlink -g"
	},
	"sideEffects": false,
	"version": "0.8.0"
}
